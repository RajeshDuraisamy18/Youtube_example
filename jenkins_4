pipeline {
    agent any

    environment{
        PATH = '/opt/apache-maven-3.9.8/bin'
    }    
    stages {
        stage('Welcome to Jenkins pipeline') {
            steps {
                echo 'Hi Rajesh !!!!'
            }
        }
        stage('Git checkout'){
           steps {
                git 'https://github.com/RajeshDuraisamy18/Youtube_example'      
           }
      
    }
        stage('Maven Build') {
            steps {
                sh 'mvn clean package'
            }
        }
    }
}
